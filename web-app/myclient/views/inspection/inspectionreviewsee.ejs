                <div class="row">
                    <div class="col-md-12">
                        <h1 class="page-header">
                            Review Details 
                        </h1>
						<!-- <ol class="breadcrumb">
  <li><a href="admindash#">Home</a></li>
  <li><a href="admindash#">Library</a></li>
  <li class="active">Data</li>
</ol> -->
                    <div class="col-md-8 col-sm-12 col-xs-12">

                        <div class="panel panel-default">
                            <div class="panel-heading">
                                Members' Current Status
                            </div> 
                            <div class="panel-body">
                                <div class="table-responsive">
                                    
                                    <table class="table table-striped table-bordered table-hover">
                                        <thead>
                                            <tr>
                                                <th>Review ID</th>
                                                <th>Project ID</th>
                                                <th>Reviewed By</th>
                                                <th>Sent To</th>
                                                <th>Sent Date</th>
                                                <th>Status</th>
                                                <th>Check</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <% for (let i = 0; i < data.length; i++){ %>
                                            <tr>
                                                    <form action="contractorreviewsee2" method="POST" role="form">
                                                    <div style="display: none;">
                                                            <input name="query" value="<%= data[i].Key %>">
                                                    </div>
                                                <td><%= data[i].Key %></td>
                                                <td><%=data[i].Record.projectId %></td>
                                                <td><%=data[i].Record.senderId %></td>
                                                <td><%=data[i].Record.receiverId %></td>
                                                <td><%=data[i].Record.sent_date %></td>
                                                <td><%=data[i].Record.status %></td>
                                                <td> <button type="submit" class="btn btn-default">Check</button></td>
                                            </form>
                                            </tr> 
                                            <% } %>   
                                        </tbody>
                                    </table>

                                </div>
                            </div>
                        </div>

                    </div>
                </div>
                
