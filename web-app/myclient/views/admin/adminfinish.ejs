        <div id="page-wrapper">
            <div id="page-inner">
                <div class="row">
                    <div class="col-md-12">
                        <h1 class="page-header">
                            Submit Project for Inspection
                        </h1>
                    </div>
                </div>
                <!-- Form -->
                <form action="http://127.0.0.1:8081/projOwnerCompleted" method="post" role="form">
                        <div class="form-group">
                                <label>Select Project</label>
                                <select name="projectId" class="form-control">
                                    <% for(var i = 0; i < dataproj.length ; i++){  %>
                                    <option value="<%= dataproj[i].Key %>" name="projectId"><%= dataproj[i].Key %></option>      
                                    <% } %>                          
                                </select>
                            </div>
                            <div class="form-group">
                                    <label>Inspection Agency</label>
                                    <select name="inspectionCoId" class="form-control">
                                        <% for(var i = 0; i < data.length ; i++){ %>
                                        <option name="inspectionCoId" value="<%= data[i].Key %>"><%= data[i].Record.companyName %></option>
                                        <% } %>
                                    </select>
                            </div>

                        <button type="submit" class="btn btn-default">Finish Project</button>
                        
                    </form>
		    </div>
